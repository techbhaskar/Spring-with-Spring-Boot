<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="iocbean" class="com.training.spring.ExplainSpringIoc">
<property name="name" value="Spring Training"></property>
</bean>

<bean id="contructorInj" class="com.training.spring.ExplainSpringDiWithCon">  
<constructor-arg value="1" type="int"></constructor-arg>
<constructor-arg value="Spring Training" type="String"></constructor-arg>
<constructor-arg value="Online" type="String"></constructor-arg>
</bean>  

<bean id="setterInj" class="com.training.spring.ExplainSpringDi">  
	<property name="id">  
	<value>1</value>  
	</property>  
	<property name="name">  
	<value>Spring Training</value>  
	</property>  
	<property name="city">  
	<value>Online</value>  
	</property>	  
</bean>  

</beans>